datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Session {
  id            String    @id @default(uuid())
  email         String
  mailTmId      String?
  adobeId      String?
  startedAt     DateTime  @default(now())
  endedAt       DateTime?
}
