datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Session {
  id            String    @id @default(uuid())
  email         String
  inboxId       String?
  startedAt     DateTime  @default(now())
  endedAt       DateTime?
  isActive      Boolean   @default(true)
}
